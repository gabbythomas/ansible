pipeline {
  agent any
  environment {
    AWS_ACCESS_KEY_ID     = credentials('AWS_ACCESS_KEY_ID')
    AWS_SECRET_ACCESS_KEY = credentials('AWS_SECRET_ACCESS_KEY')
  }
  stages {
    stage('Generate PEM file') {
      steps {
        dir ("ec2_lifecycle") {
          sh 'ls -l'
          sh 'echo $AWS_SECRET_ACCESS_KEY'
        }
      }
    }
  }
//  post {
